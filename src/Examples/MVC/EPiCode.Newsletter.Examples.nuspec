<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>EPiCode.Newsletter.Examples</id>
        <version>$version$</version>
        <authors>The EPiCode Community</authors>
        <owners>BV Network AS</owners>
        <projectUrl>https://github.com/BVNetwork/Newsletter</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Example newsletters in MVC for the Alloy sample site. This package uses features in the Alloy site, and will probably not work as expected without it.</description>
        <summary>Install and change the example as you see fit. This package should not be updated, as you will lose your changes.</summary>
        <iconUrl>http://www.coderesort.com/favicon.ico</iconUrl>
        <dependencies>
            <dependency id="EPiCode.Newsletter"
                        version="4.0.1" />
        </dependencies>
    </metadata>
    <files>
        <file src="Controllers\**\*.*" target="content\Controllers\" />
        <file src="Models\**\*.*" target="content\Models\" />
        <file src="Static\**\*.*" target="content\Static\" />
        <file src="Views\**\*.*" target="content\Views\" />
    </files>
</package>