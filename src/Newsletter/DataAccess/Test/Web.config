<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.web>
		<globalization requestEncoding="utf-8" responseEncoding="utf-8" />
		<!-- 
			Do not allow anonymous users access to plug-in
		-->
    <authorization>
      <allow roles="WebAdmins, Administrators" />
      <deny users="*" />
    </authorization>
  </system.web>
</configuration>
